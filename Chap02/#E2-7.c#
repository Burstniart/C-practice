#include <stdio.h>

int wordlenght();

int main(int argc, char *argv[]) {
  printf("%d\n", wordlenght());
  return 0;
}

unsigned rightrot(unsigned x, int n) {

  8 }
  9
 10 unsigned rightrot(unsigned x, int n) {
 11
 12 }
}

int wordlenght() {
  unsigned x = 0;
  int y;
  x = ~((~x) >> 1);
  while (x > 0) {
    x /= 2;
    ++y;
  }
  return y;
}
